<Window x:Class="FYIStockPile.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FYIStockPile"
        xmlns:mvvmc="clr-namespace:MVVMC;assembly=MVVMC"
        mc:Ignorable="d"
        Title="FYI - Migrate" Height="650" Width="950"
        FontSize="14"
        ResizeMode="NoResize">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="30"/>
            </Grid.RowDefinitions>

            <Border Grid.Row="1" Grid.ColumnSpan="2" BorderBrush="Black" BorderThickness="0,0,0,1" />

            <mvvmc:Region Grid.Row="1" ControllerID="Wizard"></mvvmc:Region>

        <Border Grid.Row="1" Grid.ColumnSpan="2" BorderBrush="Black" BorderThickness="0,1,0,0" />
    </Grid>
</Window>
